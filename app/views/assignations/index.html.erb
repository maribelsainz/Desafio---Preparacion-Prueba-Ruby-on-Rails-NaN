<p id="notice"><%= notice %></p>
<h1>Personas con proyecto asignado</h1>

<table>
<thead>
    <tr>
      <th>People</th>
      <th>Project</th>
      <th colspan="3"></th>
    </tr>
  </thead>


  <tbody>
    <tr>
     <% @people_with_project.each do |f| %>
      <td><%= f.person.name %></td>
      <td><%= f.project.name%></td>
      <td><%= link_to 'Destroy', f, method: :delete, data: { confirm: '¿Estás seguro?' } %></td>
    </tr>
    <% end %>
  </tbody>
</table>